{
	"extends": ["@financial-times/eslint-config-next", "@dotcom-reliability-kit/eslint-config"],
	"parser": "@babel/eslint-parser",
	"parserOptions": {
		"requireConfigFile": false,
		"sourceType": "script"
	},
	"plugins": ["prettier"],
	"rules": {
		"indent": "off",
		"prettier/prettier": "error",
		"quotes": 0,
		"space-before-function-paren": 0
	}
}
